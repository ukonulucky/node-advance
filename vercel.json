{
    "version": 2,
    "functions": {
        "api/*": {
"maxDuration": 60
        }
    },
    "builds":[
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"app.js"
        }
    ]

    
}